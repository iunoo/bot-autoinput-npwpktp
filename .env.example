# --- Kunci API Telegram ---
# Dapatkan dari @BotFather di Telegram
TELEGRAM_BOT_TOKEN="your_telegram_bot_token_here"

# --- PILIH LAYANAN AI YANG AKTIF ---
# Ganti nilainya menjadi "openai" atau "deepseek"
ACTIVE_AI_SERVICE="openai"

# --- Kunci API AI (pilih salah satu atau keduanya) ---
# Dapatkan dari https://platform.openai.com/api-keys
OPENAI_API_KEY="your_openai_api_key_here"
# Dapatkan dari https://platform.deepseek.com/api_keys
DEEPSEEK_API_KEY="your_deepseek_api_key_here"

# --- NAMA MODEL AI (bisa diubah jika ada model baru) ---
OPENAI_MODEL="gpt-4o-mini"
DEEPSEEK_MODEL="deepseek-vision"

# --- ID Google ---
# Buka spreadsheet Anda, ID-nya ada di URL: docs.google.com/spreadsheets/d/ID_SHEET_NYA_DI_SINI/edit
GOOGLE_SHEET_ID="your_google_sheet_id_here"

# --- PETA FOLDER GOOGLE DRIVE ---
# Dapatkan ID folder dari URL Google Drive: drive.google.com/drive/folders/ID_FOLDER_NYA_DI_SINI
FOLDER_BJ="your_bj_folder_id_here"
FOLDER_BJM="your_bjm_folder_id_here"
FOLDER_SBY="your_sby_folder_id_here"
FOLDER_SMD_BPN="your_smd_bpn_folder_id_here"
FOLDER_SMG="your_smg_folder_id_here"

# --- PETA NAMA SHEET ---
# Nama exact sheet/tab di Google Sheets Anda
SHEET_BJ="your_bj_sheet_name_here"
SHEET_SMG="your_smg_sheet_name_here"
SHEET_SBY="your_sby_sheet_name_here"
SHEET_BJM="your_bjm_sheet_name_here"
SHEET_SMD_BPN="your_smd_bpn_sheet_name_here"